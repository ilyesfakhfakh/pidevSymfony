<script>
document.getElementById('search-button').addEventListener('click', function() {
  var searchQuery = document.getElementById('dataTable_filter input[type="search"]').value;

  // Send an Ajax request to the server with the search query
  var xhr = new XMLHttpRequest();
  xhr.open('GET', '{{ path('app_mission_index') }}?search=' + searchQuery, true);

  xhr.onload = function() {
    if (xhr.status === 200) {
      // Update the table body with the new data
      var tableBody = document.getElementById('dataTable tbody');
      tableBody.innerHTML = xhr.responseText;
    }
  };

  xhr.send();
});
</script>
