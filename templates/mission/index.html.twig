						    <div class="app-card-body">
                            
							    <div class="table-responsive">
			               <table class="table app-table-hover mb-0 text-left">
                                    

										 <thead>
            <tr>
                <th>IdMission</th>
                <th>Start_date</th>
                <th>End_date</th>
                <th>Location</th>
                <th>Waste Type</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for mission in missions %}
            <tr>
                <td>{{ mission.idMission }}</td>
                <td>{{ mission.startDate ? mission.startDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ mission.endDate ? mission.endDate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ mission.location }}</td>
                <td>{{ mission.typeD }}</td>
                {% if mission.status=="completed" %}
                <td><span class="badge bg-success">{{ mission.status }}</span></td>
                {%elseif mission.status=="ongoing" %}
                <td><span class="badge bg-warning">{{ mission.status }}</span></td>
				{% endif %}
                {% if mission.status=="Undone" %}
                <td><span class="badge bg-danger">{{ mission.status }}</span></td>
                {% endif %}
                <td>
                    
                    <a href="{{ path('app_mission_edit', {'id_mission': mission.getIdMission()}) }}" class="btn-sm app-btn-secondary">edit</a>
                    <a href="{{ path('app_mission_delete', {'id_mission': mission.getIdMission()}) }}" onclick="return confirm('Are you sure you want to delete this item?');" class="btn-sm app-btn-secondary">delete</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
									</table>


                                     </div><!--//table-responsive-->
                                
						       
						    </div><!--//app-card-body-->		
						</div><!--//app-card-->
						
						<nav aria-label="Page navigation">
    <ul class="pagination justify-content-center mt-4 custom-pagination"> <!-- Utilisation de votre propre classe custom-pagination -->
        <li class="page-item {% if page == 1 %}disabled{% endif %}">
            <a class="page-link custom-link text-dark" href="{{ path('app_mission_index', {'page': page - 1}) }}">Previous</a>
        </li>
        {% for pageNumber in 1..total_pages %}
            <li class="page-item {% if page == pageNumber %}active{% endif %}">
                <a class="page-link custom-link text-dark" href="{{ path('app_mission_index', {'page': pageNumber}) }}">{{ pageNumber }}</a>
            </li>
        {% endfor %}
        <li class="page-item {% if page == total_pages %}disabled{% endif %}">
            <a class="page-link custom-link text-dark" href="{{ path('app_mission_index', {'page': page + 1}) }}">Next</a>
        </li>
    </ul>
</nav>
						
			        </div><!--//tab-pane-->    
		    </div><!--//container-fluid-->
	    </div><!--//app-content-->  
{% endblock %}
                                   
